<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_3.dtd">

<generatorConfiguration>
	<classPathEntry location="/repo/mysql/mysql-connector-java/5.1.28/mysql-connector-java-5.1.28.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!--去除注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://127.0.0.1:3306/flowable-test1?useUnicode=true&amp;characterEncoding=UTF-8"
						userId="root" password="root">
		</jdbcConnection>
	
		<!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建 使用Maven生成在target目录下，会自动创建） -->
		<javaModelGenerator targetPackage="com.fxtcn.platform.entity"
							targetProject="platform-flowable/src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaModelGenerator>
		<!--生成SQLMAP文件 -->
		<sqlMapGenerator targetPackage="mybatis.mapper"
						 targetProject="platform-flowable/src/main/resources">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<!--生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现 context id="DB2Tables" 修改targetRuntime="MyBatis3" -->
		<javaClientGenerator type="XMLMAPPER"
							 targetPackage="com.fxtcn.platform.dao" 
							 targetProject="platform-flowable/src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

	
		<table tableName="act_process_type" domainObjectName="ActProcessType"
			   enableCountByExample="false" enableDeleteByExample="fasle"
			   enableSelectByExample="false" enableUpdateByExample="false"
			   selectByExampleQueryId="fasle" />
	</context>
</generatorConfiguration>